{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2021",
    "lib": ["ES2021", "DOM"],
    "types": ["node"],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "removeComments": true,
    "noEmit": true,
  },
  "include": ["e2e/**/*.ts"],  
  "exclude": ["node_modules"]
}
